[project]
title = Defense Lane
dependencies = https://github.com/dapetcu21/defold-fmod/archive/v2.1.1.zip,https://github.com/rgrams/rendercam/archive/v1.0.1.zip
custom_resources = gamelib/fmod/banks/

[library]
include_dirs = /gamelib

[fmod]
speaker_mode = 5.1
lib_path = gamelib/fmod/res/

[physics]
gravity_z = 1.0
gravity_y = 0.0

[bootstrap]
main_collection = /main/level_loader.collectionc

[input]
game_binding = /input/game.input_bindingc
use_accelerometer = 0

[display]
width = 1280
height = 720
vsync = 0

[script]
shared_state = 1

